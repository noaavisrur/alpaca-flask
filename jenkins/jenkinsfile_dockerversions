import docker

def get_last_image_version(image_name):
    client = docker.from_env()
    image_tags = client.images.list(name=image_name)

    if len(image_tags) == 0:
        print(f"No image found with the name '{image_name}'")
        return None

    latest_version = None
    for tag in image_tags:
        for tag_name in tag.tags:
            if tag_name.startswith(image_name + ":"):
                version = tag_name[len(image_name) + 1:]
                if latest_version is None or version > latest_version:
                    latest_version = version

    if latest_version is None:
        print(f"No versions found for the image '{image_name}'")
    else:
        print(f"The latest version of '{image_name}' is '{latest_version}'")

    return latest_version

# Example usage
image_name = "my-docker-image"
last_version = get_last_image_version(image_name)

